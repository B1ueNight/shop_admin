<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bluenight.shoppingmall_admin.mapper.OrderMapper">
    <select id="getOrderList" resultType="com.bluenight.shoppingmall_admin.data.OrderInfoVO">
        select * from order_info
        order by oi_seq desc
        limit 10 offset #{offset}
    </select>
    <select id="getOrderCnt" resultType="java.lang.Integer">
        select count(*) from order_info
    </select>
    </mapper>